{
  "name": "client",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng s -o",
    "build-prod": "ng build --prod",
    "dev-server": "nodemon server/main.js",
    "server": "node server/main.js",
    "electron": "electron .",
    "electron-run": "ng build && electron .",
    "electron-build": "ng build --prod && electron .",
    "electron-generate": "ng build --prod && electron-packager . \"File Manager\" --all --out='../build' --overwrite --icon=\"./src/icon.icns\"",
    "electron-generate-win": "ng build --prod && electron-packager . \"File Manager\" --platform='win32' --arch='x64' --asar --out='../build' --overwrite --icon=\"./src/icon.ico\"",
    "electron-generate-linux": "ng build --prod && electron-packager . \"File Manager\" --platform='linux' --arch='x64' --asar --out='../build' --overwrite --icon=\"./src/icon.png\"",
    "prod": "ng build --prod --output-path ./server/dist && node server/main.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "chalk": "^4.1.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^13.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
